<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <ImageButton
            android:layout_width="71dp"
            android:layout_height="61dp" app:srcCompat="@android:drawable/ic_partial_secure"
            android:id="@+id/imageButton8"
            tools:layout_editor_absoluteX="13dp"
            android:layout_marginBottom="24dp" app:layout_constraintBottom_toTopOf="@+id/imageButton9"/>
    <ImageButton
            android:layout_width="69dp"
            android:layout_height="63dp" app:srcCompat="@android:drawable/ic_partial_secure"
            tools:layout_editor_absoluteY="512dp" tools:layout_editor_absoluteX="17dp" android:id="@+id/imageButton9"/>
    <ImageButton
            android:layout_width="70dp"
            android:layout_height="61dp" app:srcCompat="@android:drawable/ic_dialog_email"
            tools:layout_editor_absoluteY="220dp" tools:layout_editor_absoluteX="11dp" android:id="@+id/imageButton11"/>
    <ImageButton
            android:layout_width="66dp"
            android:layout_height="71dp" app:srcCompat="@android:drawable/stat_sys_vp_phone_call"
            android:id="@+id/imageButton13"
            tools:layout_editor_absoluteY="312dp" tools:layout_editor_absoluteX="11dp"/>
    <EditText
            android:layout_height="wrap_content"
            android:ems="10"
            tools:layout_editor_absoluteY="52dp" tools:layout_editor_absoluteX="129dp" android:id="@+id/etpassword"
            android:hint=" First Name"/>
    <EditText
            android:layout_width="LastName"
            android:layout_height="42dp"
            android:inputType="textPersonName|textPostalAddress"
            android:ems="10"
            tools:layout_editor_absoluteY="145dp" android:id="@+id/etpassword"
            android:hint="Last Name" tools:layout_editor_absoluteX="130dp"/>
    <EditText
            android:layout_width="Email"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            tools:layout_editor_absoluteY="235dp" tools:layout_editor_absoluteX="129dp" android:id="@+id/tvpassword"
            android:hint="Email "/>
    <EditText
            android:layout_width="PhoneNumber"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            tools:layout_editor_absoluteY="338dp" tools:layout_editor_absoluteX="129dp" android:id="@+id/etpassword"
            android:hint="Phone Number"/>
    <EditText
            android:layout_width="password"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            tools:layout_editor_absoluteY="442dp" tools:layout_editor_absoluteX="129dp" android:id="@+id/etpassword"
            android:hint="password"/>
    <EditText
            android:layout_width="confirmpassword"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            tools:layout_editor_absoluteY="529dp" tools:layout_editor_absoluteX="130dp" android:id="@+id/etpassword"
            android:hint="confirm password"/>
    <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="610dp"
            tools:layout_editor_absoluteX="178dp" android:id="@+id/Btn" tools:text="signin"/>
    <ImageView
            android:layout_width="70dp"
            android:layout_height="65dp" tools:srcCompat="@tools:sample/avatars"
            tools:layout_editor_absoluteY="27dp" tools:layout_editor_absoluteX="14dp" android:id="@+id/imageView2"/>
    <ImageView
            android:layout_width="75dp"
            android:layout_height="63dp" tools:srcCompat="@tools:sample/avatars"
            tools:layout_editor_absoluteY="124dp" tools:layout_editor_absoluteX="14dp" android:id="@+id/imageView5"/>
</android.support.constraint.ConstraintLayout>